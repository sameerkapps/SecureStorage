<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="3.0.0">
    <id>Xam.Plugin.SecureStorage</id>
    <version>$version$</version>
    <title>Secure Storage Plugin for Xamarin and Windows</title>
    <authors>Alberto Fustinoni</authors>
    <owners>Alberto Fustinoni</owners>
    <licenseUrl>https://github.com/aftnet/SecureStoragePlugin/blob/master/README.md</licenseUrl>
    <projectUrl>https://github.com/aftnet/SecureStoragePlugin</projectUrl>
    <iconUrl>https://github.com/Aftnet/SecureStoragePlugin/raw/master/Icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Provides Secure storage of key value pairs for iOS, Android and UWP apps.</description>
    <summary>Secure storage of key value pairs for iOS, Android and UWP apps.</summary>
    <tags>xamarin, plugin, plugin for xamarin, android, xamarin.forms, ios, secure, storage, UWP</tags>
    <dependencies>
      <group targetFramework="net46">
      </group>
      <group targetFramework="netstandard1.4">
        <dependency id="NETStandard.Library" version="1.6.1" />
      </group>
      <group targetFramework="uap10.0">
        <dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="5.1.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!--Core-->
    <file src="Plugin.SecureStorage\bin\$configuration$\netstandard1.0\Plugin.SecureStorage.*" target="lib\netstandard1.0" />
    <!--UWP-->
    <file src="Plugin.SecureStorage.UWP\bin\$configuration$\Plugin.SecureStorage.*" target="lib\uap10.0" />
    <!--Windows Desktop-->
    <file src="Plugin.SecureStorage.Net46\bin\$configuration$\Plugin.SecureStorage.*" target="lib\net46" />
    <!--Xamarin.Android-->
    <file src="Plugin.SecureStorage.Android\bin\$configuration$\Plugin.SecureStorage.*" target="lib\MonoAndroid10" />
    <!--Xamarin.iOS Unified-->
    <file src="Plugin.SecureStorage.iOS\bin\$configuration$\Plugin.SecureStorage.*" target="lib\Xamarin.iOS10" />
  </files>
</package>